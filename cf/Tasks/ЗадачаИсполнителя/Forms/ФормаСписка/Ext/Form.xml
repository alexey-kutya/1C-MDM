			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ЗадачаЗадачаИсполнителя.Ссылка,
	ЗадачаЗадачаИсполнителя.ПометкаУдаления,
	ЗадачаЗадачаИсполнителя.Номер,
	ЗадачаЗадачаИсполнителя.Дата,
	ЗадачаЗадачаИсполнителя.БизнесПроцесс,
	ЗадачаЗадачаИсполнителя.ТочкаМаршрута,
	ЗадачаЗадачаИсполнителя.Наименование,
	ЗадачаЗадачаИсполнителя.Выполнена,
	ЗадачаЗадачаИсполнителя.Важность,
	ЗадачаЗадачаИсполнителя.ДатаИсполнения,
	ЗадачаЗадачаИсполнителя.Автор,
	ЗадачаЗадачаИсполнителя.Описание,
	ЗадачаЗадачаИсполнителя.СрокИсполнения,
	ЗадачаЗадачаИсполнителя.СрокОповещения,
	ЗадачаЗадачаИсполнителя.РезультатВыполнения,
	ЗадачаЗадачаИсполнителя.ПредметСтрокой,
	ЗадачаЗадачаИсполнителя.Исполнитель,
	ЗадачаЗадачаИсполнителя.РольИсполнителя,
	ЗадачаЗадачаИсполнителя.ОсновнойОбъектАдресации,
	ЗадачаЗадачаИсполнителя.ДополнительныйОбъектАдресации,
	ЗадачаЗадачаИсполнителя.ДатаНачала,
	ЗадачаЗадачаИсполнителя.ДлительностьИсполнения,
	ЗадачаЗадачаИсполнителя.Результат,
	ЗадачаЗадачаИсполнителя.ПринятаКИсполнению
ИЗ
	Задача.ЗадачаИсполнителя КАК ЗадачаЗадачаИсполнителя
ГДЕ
	(&amp;Исполнитель ЕСТЬ NULL 
			ИЛИ ЗадачаЗадачаИсполнителя.Исполнитель = &amp;Исполнитель
			ИЛИ ЗадачаЗадачаИсполнителя.СпособРаспределения = ЗНАЧЕНИЕ(Перечисление.нсиСпособыРаспределенияЗадач.ПоРолиИГруппеПользователейБП)
				И ЗадачаЗадачаИсполнителя.Исполнитель = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
				И (ЗадачаЗадачаИсполнителя.РольИсполнителя, ЗадачаЗадачаИсполнителя.ОсновнойОбъектАдресации) В
					(ВЫБРАТЬ
						РегистрСведений.ИсполнителиЗадач.РольИсполнителя,
						РегистрСведений.ИсполнителиЗадач.ОсновнойОбъектАдресации
					ИЗ
						РегистрСведений.ИсполнителиЗадач
					ГДЕ
						РегистрСведений.ИсполнителиЗадач.Исполнитель = &amp;Исполнитель)
			ИЛИ ЗадачаЗадачаИсполнителя.СпособРаспределения = ЗНАЧЕНИЕ(Перечисление.нсиСпособыРаспределенияЗадач.ПоРоли)
				И ЗадачаЗадачаИсполнителя.Исполнитель = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
				И ЗадачаЗадачаИсполнителя.РольИсполнителя В
					(ВЫБРАТЬ
						РегистрСведений.ИсполнителиЗадач.РольИсполнителя
					ИЗ
						РегистрСведений.ИсполнителиЗадач
					ГДЕ
						РегистрСведений.ИсполнителиЗадач.Исполнитель = &amp;Исполнитель)
			ИЛИ ЗадачаЗадачаИсполнителя.СпособРаспределения = ЗНАЧЕНИЕ(Перечисление.нсиСпособыРаспределенияЗадач.ПоГруппеПользователейБП)
				И ЗадачаЗадачаИсполнителя.Исполнитель = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
				И ЗадачаЗадачаИсполнителя.ОсновнойОбъектАдресации В
					(ВЫБРАТЬ
						РегистрСведений.ИсполнителиЗадач.ОсновнойОбъектАдресации
					ИЗ
						РегистрСведений.ИсполнителиЗадач
					ГДЕ
						РегистрСведений.ИсполнителиЗадач.Исполнитель = &amp;Исполнитель)
			ИЛИ &amp;ОтображениеРасшифровки)</QueryText>
				<MainTable>Task.ЗадачаИсполнителя</MainTable>
