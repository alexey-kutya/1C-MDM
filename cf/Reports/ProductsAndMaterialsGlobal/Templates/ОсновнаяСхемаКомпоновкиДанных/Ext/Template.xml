	GlobalSKU.MaterialType.Код,
	GlobalSKU.Packing,
	GlobalSKU.Packing.Код,
	GlobalSKU.Volume.SizeNumber,
	GlobalSKU.Volume.Код,
	GlobalSKU.Version,
	GlobalSKU.VersionDescription,
	MappingGlobalAndLocalSKU.LocalItem.СтранаОрганизации
