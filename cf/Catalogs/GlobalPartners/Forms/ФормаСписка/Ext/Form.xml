			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	СправочникНСИ.Ссылка,
	СправочникНСИ.ПометкаУдаления,
	СправочникНСИ.Предопределенный,
	СправочникНСИ.Код,
	СправочникНСИ.Наименование,
	СправочникНСИ.ЭтоМакет,
	СтатусыОбработки.Пользователь КАК Пользователь,
	СтатусыОбработки.ВременныйЭлемент КАК ВременныйЭлемент,
	СтатусыОбработки.СозданаЗаявка КАК СозданаЗаявка,
	СтатусыОбработки.ОбработкаНачата КАК ОбработкаНачата,
	СтатусыОбработки.Обработано КАК Обработано,
	СтатусыОбработки.Обработавший КАК Обработавший,
	СправочникНСИ.LocalID,
	СправочникНСИ.ID,
	СправочникНСИ.Type,
	СправочникНСИ.ShortName,
	СправочникНСИ.Entity,
	СправочникНСИ.Intercompany,
	СправочникНСИ.IsSupplier,
	СправочникНСИ.IsCustomer,
	СправочникНСИ.IsNonTrade,
	СправочникНСИ.Market,
	СправочникНСИ.DeliveryAddress,
	СправочникНСИ.Currency,
	СправочникНСИ.Status,
	СправочникНСИ.Incoterms,
	СправочникНСИ.СтранаОрганизации
ИЗ
	Справочник.GlobalPartners КАК СправочникНСИ
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.нсиСтатусыОбработкиСправочников КАК СтатусыОбработки
		ПО (СтатусыОбработки.Объект = СправочникНСИ.Ссылка)
			И (ВЫБОР
				КОГДА &amp;СсылкаНеОпределена
					ТОГДА ИСТИНА
				ИНАЧЕ СправочникНСИ.Ссылка В (&amp;Ссылка)
			КОНЕЦ)
			И (ВЫБОР
				КОГДА &amp;СсылкаНеОпределена
					ТОГДА ИСТИНА
				ИНАЧЕ СтатусыОбработки.Объект В (&amp;Ссылка)
			КОНЕЦ)
			И (СтатусыОбработки.Пользователь = &amp;Пользователь
					И СтатусыОбработки.ВременныйЭлемент
				ИЛИ СтатусыОбработки.Пользователь В (&amp;ПользователиГруппыДоступа)
					И СтатусыОбработки.ВременныйЭлемент
				ИЛИ &amp;ЭтоИсполнительРоли
					И СтатусыОбработки.ВременныйЭлемент
				ИЛИ НЕ СтатусыОбработки.ВременныйЭлемент)</QueryText>
				<MainTable>Catalog.GlobalPartners</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
		</Attribute>
