	СправочникНСИ.MaterialGroup,
	СправочникНСИ.MaterialType,
	СправочникНСИ.Packing,
	СправочникНСИ.Version,
	СправочникНСИ.VersionDescription,
	СправочникНСИ.Volume,
	СправочникНСИ.ID,
	СправочникНСИ.LocalName,
	СправочникНСИ.LocalID,
	СправочникНСИ.СтранаОрганизации
ИЗ
	Справочник.GlobalSKU КАК СправочникНСИ
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.нсиСтатусыОбработкиСправочников КАК СтатусыОбработки
		ПО (СтатусыОбработки.Объект = СправочникНСИ.Ссылка)
			И (ВЫБОР
				КОГДА &amp;СсылкаНеОпределена
					ТОГДА ИСТИНА
				ИНАЧЕ СправочникНСИ.Ссылка В (&amp;Ссылка)
			КОНЕЦ)
			И (ВЫБОР
				КОГДА &amp;СсылкаНеОпределена
					ТОГДА ИСТИНА
				ИНАЧЕ СтатусыОбработки.Объект В (&amp;Ссылка)
			КОНЕЦ)
			И (СтатусыОбработки.Пользователь = &amp;Пользователь
					И СтатусыОбработки.ВременныйЭлемент
				ИЛИ СтатусыОбработки.Пользователь В (&amp;ПользователиГруппыДоступа)
					И СтатусыОбработки.ВременныйЭлемент
				ИЛИ &amp;ЭтоИсполнительРоли
					И СтатусыОбработки.ВременныйЭлемент
				ИЛИ НЕ СтатусыОбработки.ВременныйЭлемент)</QueryText>
				<MainTable>Catalog.GlobalSKU</MainTable>
